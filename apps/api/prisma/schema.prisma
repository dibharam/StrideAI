generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String
  tz        String   @default("Europe/London")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
